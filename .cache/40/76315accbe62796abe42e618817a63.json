et {\r\n  background: white; //#171717 - dark\r\n  height: 450px;\r\n  width: 50em;\r\n  margin-top: 2.85rem;\r\n  @include flex-center(row, space-between);\r\n}\r\n\r\n.quote {\r\n  font-size: 1.1rem;\r\n  width: 700px;\r\n  text-align: center;\r\n}\r\n\r\n.greet-name {\r\n  font-size: 2.7rem;\r\n  width: 100%;\r\n  font-family: \"Poppins\", sans-serif;\r\n  background: none;\r\n  border: none;\r\n  outline: none;\r\n  color: white;\r\n  text-transform: capitalize;\r\n  &::placeholder {\r\n    color: rgb(185, 185, 185);\r\n  }\r\n}\r\n\r\n.input-hover {\r\n  cursor: pointer;\r\n}\r\n\r\n.notes-section-left {\r\n  width: 40em;\r\n  height: 100%;\r\n  @include flex-center(column, space-between);\r\n  .editor {\r\n    width: 100%;\r\n    height: 85%;\r\n    overflow-y: scroll;\r\n    .note-heading {\r\n      width: 100%;\r\n      font-family: \"Poppins\", sans-serif;\r\n      font-size: 2.4rem;\r\n      padding: 0.5rem 1rem;\r\n      outline: none;\r\n      border: none;\r\n      font-weight: 500;\r\n      position: -webkit-sticky; /* Safari */\r\n      position: sticky;\r\n      top: 0;\r\n      border-bottom: 1px solid #a3a3a3;\r\n    }\r\n    .note-content {\r\n      color: black;\r\n      padding: 0.5rem 1rem 2rem 1rem;\r\n      outline: none;\r\n      font-size: 1.1rem;\r\n    }\r\n  }\r\n  .options {\r\n    height: 15%;\r\n    width: 100%;\r\n    border-top: 1px solid #a3a3a3;\r\n    @include flex-center(row, space-between);\r\n    padding: 0 1rem;\r\n    .btn-list {\r\n      @include flex-center(row, space-between);\r\n      width: 14rem;\r\n      .options-btn {\r\n        background: #979797;\r\n        border: none;\r\n        color: white;\r\n        font-size: 1.4rem;\r\n        width: 40px;\r\n        height: 40px;\r\n        font-family: \"Poppins\", sans-serif;\r\n        cursor: pointer;\r\n        outline: none;\r\n      }\r\n      .bold {\r\n        font-weight: 600;\r\n      }\r\n      .italic {\r\n        font-family: serif;\r\n        font-style: italic;\r\n        font-weight: 500;\r\n      }\r\n      .underline {\r\n        text-decoration: underline;\r\n      }\r\n      .striked {\r\n        text-decoration: line-through;\r\n        font-weight: 300;\r\n      }\r\n      .list {\r\n        @include flex-center(row, center);\r\n        img {\r\n          pointer-events: none;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.notes-section-right {\r\n  height: 100%;\r\n  width: 15em;\r\n  background: #ededed;\r\n}\r\n\r\n.active {\r\n  background: rgb(82, 82, 82) 000 !important;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"7620b97cfd7a74cd60ce2dbfe8bf87bb","cacheData":{"env":{}}}